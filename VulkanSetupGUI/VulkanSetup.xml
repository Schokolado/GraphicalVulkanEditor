<VulkanSetup>
    <instance>
        <applicationNameInput name="applicationNameInput">Test Application</applicationNameInput>
        <showValidationLayerDebugInfoCheckBox name="showValidationLayerDebugInfoCheckBox">VK_TRUE
        </showValidationLayerDebugInfoCheckBox>
        <runOnMacosCheckBox name="runOnMacosCheckBox">VK_FALSE</runOnMacosCheckBox>
    </instance>
    <physicalDevice>
        <chooseGPUOnStartupCheckBox name="chooseGPUOnStartupCheckBox">VK_FALSE</chooseGPUOnStartupCheckBox>
    </physicalDevice>
    <logicalDevice>
        <deviceExtensionsList name="deviceExtensionsList">['VK_KHR_SWAPCHAIN_EXTENSION_NAME',
            'VK_KHR_SWAPCHAIN_EXTENSION_NAME test']
        </deviceExtensionsList>
    </logicalDevice>
</VulkanSetup>